block head
	link(rel="stylesheet", href="css/progressBar.css")
div.row

	div.progress.skill-bar
		div(class="progress-bar progress-bar-success" role="progressbar" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100")
			span.skill HTML <i class="val">90%</i>

	div.progress.skill-bar
		div(class="progress-bar progress-bar-info" role="progressbar" aria-valuenow="90" aria-valuemin="0" aria-valuemax="100")
			span.skill HTML <i class="val">90%</i>

	div.progress.skill-bar
		div(class="progress-bar progress-bar-warning" role="progressbar" aria-valuenow="30" aria-valuemin="0" aria-valuemax="100")
			span.skill HTML <i class="val">30%</i>

	div.progress.skill-bar
		div(class="progress-bar progress-bar-success" role="progressbar" aria-valuenow="50" aria-valuemin="0" aria-valuemax="100")
			span.skill HTML <i class="val">50%</i>